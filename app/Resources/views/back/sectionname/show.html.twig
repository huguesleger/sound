{% extends ':back:templateAdmin.html.twig' %}

  {% block content %}
      
    <h1>Sectionname</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sectionName.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sectionName.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sectionname_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('sectionname_edit', { 'id': sectionName.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
